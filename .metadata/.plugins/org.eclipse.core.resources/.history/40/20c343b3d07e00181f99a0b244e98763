
public class StockApp {

	public static void main(String[] args) {
		double v52WeeksHigh=23.89;
		double v52WeeksLow=21.49;
		double Gap=v52WeeksHigh-v52WeeksLow;
		double vCurrentPrice=22.00;
		
		if( vCurrentPrice< vCurrentPrice+Gap){
			System.out.println("Please Buy");
		}else{
			System.out.println("Do Not Buy");
		}

	}

}
